{
  "manifest_version": 2,
  "name": "Simply Proxy",
  "description": "一个朴素的代理配置扩展",
  "minimum_chrome_version": "22",
  "version": "0.1.8",
  "icons": {
    "16"  : "icons/icon_16.png",
    "128" : "icons/icon_128.png"
  },
  "options_page": "setting.html",
  "browser_action": {
    "default_icon": "icons/icon_32.png",
    "default_title": "Simple Proxy",
    "default_popup": "popup.html"
  },
  "permissions": [
    "proxy",
    "storage"
  ]
}
